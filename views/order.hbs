<h1>Order</h1>

{{#if order.length}}
    {{#each order}}
        <div class="row">
            <div class="col s6 offset-s3">
                <div class="card">
                    <div class="card-content">
                <span class="card-title">
                    Order <smal>{{_id}}</smal>
                </span>
                        <p>{{date}}</p>
                        <p><em>{{user.userId.name}}</em> {{user.userId.email}}</p>
                        <ol>

                        </ol>
                        <hr>
                        <p>Price: <span class="price"></span></p>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
{{else}}
    <p>There`s no orders</p>
{{/if}}
